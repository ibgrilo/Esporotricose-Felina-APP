{
  "expo": {
    "name": "EsporoVet",
    "slug": "esporoVet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo/iconAPP.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/logo/iconAPP.png",
      "resizeMode": "cover",
      "backgroundColor": "#f2f2f2"
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.esporovet.app",
      "icon": "./src/assets/images/logo/iconAPP.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo/adaptive-iconAPP.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "image": "./src/assets/images/logo/iconAPP.png",
        "resizeMode": "cover",
        "backgroundColor": "#f2f2f2"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAdF6NRC6czSt3vhoz8sH0lGtPd0EIK-lA"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esporovet.app",
      "splash": {
        "image": "./src/assets/images/logo/iconAPP.png",
        "resizeMode": "cover",
        "backgroundColor": "#f2f2f2"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Este app precisa da sua localização para mostrar crematórios próximos."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAdF6NRC6czSt3vhoz8sH0lGtPd0EIK-lA"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./src/assets/images/logo/iconAPP.png"
    },
    "extra": {
      "eas": {
        "projectId": "efe478ee-8ca6-495c-85d1-499415fe323f"
      }
    },
    "owner": "ibgrilo",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ]
  }
}