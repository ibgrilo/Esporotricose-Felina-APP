{
  "expo": {
    "name": "EsporoVet",
    "slug": "esporoVet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo/iconAPP.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/logo/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F2F2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esporovet.app",
      "splash": {
        "image": "./src/assets/images/logo/splashScreen.png",
        "resizeMode": "contain",
        "backgroundColor": "#F2F2F2"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo/adaptive-iconAPP.png",
        "backgroundColor": "#f5f5f5"
      },
      "package": "com.esporovet.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "splash": {
        "image": "./src/assets/images/logo/splashScreen.png",
        "resizeMode": "contain",
        "backgroundColor": "#F2F2F2"
      }
    },
    "web": {
      "favicon": "./src/assets/images/logo/iconAPP.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que $(PRODUCT_NAME) use sua localização para mostrar clínicas veterinárias próximas."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logo/splashScreen.png",
          "resizeMode": "contain",
          "backgroundColor": "#F2F2F2"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "706c2ea6-e249-4ad3-88f4-466b06d78400"
      }
    }
  }
}