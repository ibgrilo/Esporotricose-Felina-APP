{
  "expo": {
    "name": "EsporoVet",
    "slug": "esporoVet",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.esporovet.app",
      "icon": "./src/assets/images/logo/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "targetSdkVersion": 35,
      "compileSdkVersion": 35,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAdF6NRC6czSt3vhoz8sH0lGtPd0EIK-lA"
        }
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.esporovet.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Este app precisa da sua localização para mostrar crematórios próximos."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAdF6NRC6czSt3vhoz8sH0lGtPd0EIK-lA"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./src/assets/images/logo/icon.png"
    },
    "owner": "tayigu",
    "plugins": [
      "expo-system-ui",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#f2f2f2",
          "image": "./src/assets/images/logo/splashIconLight.png",
          "dark": {
            "image": "./src/assets/images/logo/splashIconDark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f04cbded-e4f6-4406-86b2-2c929f5c3d33"
      }
    }
  }
}